<category>
	<pattern>Donde esta *</pattern>
	<template>No se</template>
</category>


<category>
	<pattern>Como esta *</pattern>
	<template>No se</template>
</category>


<category>
	<pattern>* esta ^</pattern>
	<that>No se</that>
	<template>
		<think>
		<set var="lugar"><srai>Donde esta <star/></srai></set>
		</think>
		<condition var="lugar">
		<li value="No se">
			<learn>
				<category>
				<pattern>
					Donde esta <eval><star/></eval>
				</pattern>
				<template>
					<eval><star/></eval> esta <eval><star index="2"/></eval>
				</template>
				</category>
			</learn>
			Recordare que <star/> esta <star index="2"/>.
		</li>
		</condition>
	</template>
</category>

<category>
	<pattern>* esta ^</pattern>
	<that>No se</that>
	<template>
		<think>
		<set var="estado"><srai>Como esta <star/></srai></set>
		</think>
		<condition var="estado">
		<li value="No se">
			<learn>
				<category>
				<pattern>
					Como esta <eval><star/></eval>
				</pattern>
				<template>
					<eval><star/></eval> esta <eval><star index="2"/></eval>
				</template>
				</category>
			</learn>
			Recordare que <star/> esta <star index="2"/>.
		</li>
		</condition>
	</template>
</category>

<category>
<pattern>No</pattern>
<template>
Vaya.
</template>
</category>

<category>
<pattern>$envia a * de tema * el mensaje: *</pattern>
<template>
	<mail>
		<subject><star index="2"/></subject>
		<to><star/>@mail.gal</to>
		<msg><star index="3"/></msg>
	</mail>
</template>
</category>



<category>
<pattern>$envia el siguiente mensaje sobre el tema * a * : *</pattern>
<template>
	<mail>
		<subject><star/></subject>
		<to><star index="2"/>@mail.gal</to>
		<msg><star index="3"/></msg>
	</mail>
</template>
</category>


<!-- Respuesta por defecto -->
<category>
<pattern>*</pattern>
<template>
Puedes repetir porfavor, no te entiendo.
</template>
